#!../../ressources/yasl_linux

3 < ? ( 0 exit )
!

0:eval:2	// push av[1] on stack 2

0:#:1		// count
1:@			// while count != 0
(
	2:=:0	// dup i on top of stack 0
	0:#(0:=:3 3:print)	// print i time av[x]
	0:!
	"\n":0	// add "\n" to stack 0
	0:print	// print "\n"
	1:2		// add 1 to stack 2
	2:+:2	// sub 1 to count
	0:#:1	// recount for while loop
)
