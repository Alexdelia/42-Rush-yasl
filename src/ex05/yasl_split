#!../../ressources/yasl_linux

3 != ? ( 0 exit )
!

[#]:1 1:1 1:!=:1 1:? ( "only 1 char alowed to split\n":0 0:print 0:0 0:exit )

0:=:7 0:!	// separator
0:=:8		// string
0:6			// int i
0:3			// bool start

{
	6:= 8:[#] <			// if (i < size
	3:= && ?			//	&& bool start == true)
		("\n" print)		// print "\n"
	: (					// else
		6:= 8:[#] >=		// if (i >= size
		3:= 0 ==			//	&& bool start == false)
		&& ?				// do the &&
			(0 exit)			// (when string is only sep char)
	)
} print_nl

{
	@(					// while (str[i] == sep)
		1:6 6:+:6			// i++
		8:= 6:= %]#[:7		// copy str[i] to s7
		7:%==				// while (str[i] == sep)
		6:= 8:[#] <			// && (i < size)
		&&					// do the &&
		7:!					// pop copy
	)
	2:print_nl:2
} sep

{
	8:= 6:= %]#[:7		// copy str[i] to s7
	7:%== %?			// if (str[i] == sep)
	( 2:sep:2 )				// do sep
	: (					// else
		8:= 6:= ]#[ print	// print char
		1:6 6:+:6			// i++
	)
	7:!					// pop copy (line 13) //

} if_sep

2:if_sep:2				// skip first sep char
1:3						// bool start = true
6:=:2 8:[#]:2 2:<:1		// while (i < size)
1:@
(
	2:if_sep:2
	6:=:2 8:[#]:2 2:<:1	// while (i < size)
)

"\n":0 0:print
