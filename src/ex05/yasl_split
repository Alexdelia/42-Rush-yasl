#!../../ressources/yasl_linux

3 != ? ( 0 exit )
!

[#]:1 1:1 1:!=:1 1:? ( "only 1 char alowed to split\n":0 0:print 0:0 0:exit )

0:=:7 0:!	// separator
0:=:8		// string
0:6			// int i
0:3			// bool start

{
	8:=:2 6:=:2 2:%]#[:7		// copy str[i] to s7
	7:%==:2 2:%?				// if (str[i] == sep)
	(
		2:@(					// while (str[i] == sep)
			1:6 6:+:6				// i++
			8:=:2 6:=:2 2:%]#[:7	// copy str[i] to s7
			7:%==:2					// while (str[i] == sep)
			6:=:2 8:[#]:2 2:<:2		// && (i < size)
			2:&&:2					// do the &&
			7:!						// pop copy
		)
		6:=:2 8:[#]:2 2:<:2		// if (i < size
		3:=:2 2:&&:2 2:?		//	&& bool start == true)
			("\n":0 0:print)		// print "\n"
	)
	: (							// else
		8:=:2 6:=:2 2:]#[:2 2:print	// print char
		1:6 6:+:6					// i++
	)
	7:!							// pop copy of str[i] in s7 (line 13)
} sep

sep								// skip first sep char
1:3								// bool start = true
6:=:2 8:[#]:2 2:<:1				// while (i < size)
1:@
(
	sep
	6:=:2 8:[#]:2 2:<:1			// while (i < size)
)

"\n":0 0:print
// test sep at start
// test sep at end
