#!../../ressources/yasl_linux

#!

{	// could be bugged for / 4 if approx
	=:2			// n char
	4:2 2:/:2	// 
	1:3			// stockage
	1:4			// bool

	4:%@
	(
		3:=:3			// 1 1
		3:%*:3			// 1 1 1
		2:=:3			// 1 1 1 1024
		3:%==:3			// (1 == 1024)
		3:?	(0:4 "square of ":0 0:print 3:!!! 3:print)		// if (square == char) bool == 0
		: (				// else
		3:%>:3			// (1 > 1024)
		? 0:("not square\n" print 0 exit):0	// error
		3:!!!			// 1
		1:3				// 1 1
		3:+:3			// 1 + 1 = 2	// i++
		)
	)
	2:!
} is_square

{
	""
	read
	@(
		+
		read
	)
	!
} open

open
[#]
0:is_square

"hey\n" print
